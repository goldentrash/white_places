{
  "name": "white_places",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/goldentrash/netlify_test.git",
  "author": "goldentrash <startoflateness@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start:dev": "yarn run check && yarn run build && yarn netlify dev",
    "test:unit:api": "yarn mocha api/test/**.spec.ts --require ts-node/register",
    "build": "yarn run build:client && yarn run build:api",
    "build:client": "yarn elm make client/Main.elm --optimize --output=dist/index.html",
    "build:api": "yarn webpack",
    "lint:api": "yarn eslint api --output-file report/lint.html --format html",
    "format:api": "yarn prettier --write api",
    "check": "yarn run lint:api && yarn run test:unit:api && yarn run format:api"
  },
  "dependencies": {
    "colors": "^1.4.0",
    "git-branch-is": "^4.0.0",
    "graphql": "^15.5.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.2.0",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "chai": "^4.3.0",
    "elm": "^0.19.1-5",
    "elm-test": "^0.19.1-revision6",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "husky": "^4.3.8",
    "mocha": "^8.2.1",
    "netlify-cli": "2.54.0",
    "prettier": "^2.2.1",
    "ts-loader": "^8.0.15",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "webpack": "^5.20.2",
    "webpack-cli": "^4.5.0"
  }
}
