{
  "name": "white_places",
  "version": "1.0.0",
  "private": true,
  "repository": "https://github.com/goldentrash/netlify_test.git",
  "author": "goldentrash <startoflateness@gmail.com>",
  "scripts": {
    "prebuild": "yarn install",
    "postinstall": "yarn run gql-codegen",
    "start": "npm-run-all --parallel start:*",
    "start:client": "netlify dev",
    "start:functions": "webpack watch --config webpack/functions.config.js",
    "build": "npm-run-all --parallel build:*",
    "build:client": "webpack --mode=production --config webpack/client.config.js",
    "build:functions": "webpack --mode=production --config webpack/functions.config.js",
    "gql-codegen": "graphql-codegen --config .graphqlrc",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@apollo/client": "^3.3.12",
    "@material-ui/core": "^4.11.3",
