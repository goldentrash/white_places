{
  "name": "white_places",
  "version": "1.0.0",
  "private": true,
  "repository": "https://github.com/goldentrash/netlify_test.git",
  "author": "goldentrash <startoflateness@gmail.com>",
  "scripts": {
    "postversion": "echo \"git push origin master\"",
    "start": "npm-run-all --parallel watch start:dev",
    "start:dev": "netlify dev",
    "watch": "npm-run-all --parallel watch:*",
    "watch:client": "webpack watch --config webpack/client/development.config.js",
    "watch:lambda": "webpack watch --config webpack/lambda/development.config.js",
    "prebuild": "yarn run gql-codegen",
    "build": "yarn run build:client && yarn run build:lambda",
    "build:client": "webpack --config webpack/client/production.config.js",
    "build:lambda": "webpack --config webpack/lambda/production.config.js",
    "gql-codegen": "graphql-codegen --config .graphqlrc.yaml",
    "type-check": "tsc --noEmit",
    "test": "echo \"have no test yet\""
  },
  "dependencies": {
    "@apollo/client": "^3.3.12",
    "@graphql-codegen/cli": "1.21.1",
    "@graphql-codegen/typescript": "1.21.0",
